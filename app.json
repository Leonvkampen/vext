{
  "expo": {
    "name": "vitalis",
    "slug": "vitalis",
    "scheme": "vitalis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.vitalis"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "enabled": false
    },
    "plugins": [
      "expo-sqlite",
      "expo-router"
    ]
  }
}
